# Les lignes de commentaires commencent par #
procedure principale()
  i = 12+3;
  si (i>14)
    j=1;
  finsi
tantque(j<42)
    j=j+1;
 fintantque
repeter
j=j-10;
jusqua ( j<-1000 )
i=0;
k=0;
#penser a reparer le pour, finir le ecrire et lire
pour(i=0;i<10;)
k=k-1000;
i=i+1;
ecrire(i)
finpour
finproc
